package Motorbike;

public class Motorbike1 {
	String Bikemodel;
	int year;
	
	public Motorbike1(){
		System.out.println("this is the bike object");
	}
	public Motorbike2(){
		this.Bikemodel = Bikemodel;
		System.out.println(this.Bikemodel);
	}
	public Motorbike1(String Bikemodel,int year){
		this.Bikemodel = Bikemodel;
		this.year = year;
		System.out.println(this.Bikemodel+"  "+this.year);
	}

}
